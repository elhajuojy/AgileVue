{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
        "baseUrl": "./resources/js/",
        "paths": {
            "@/*": ["./*"]
        },
        "strict": true,
      "esModuleInterop": true,
        "types": [
            "vue-sweetalert2"
        ]
    },

    "include": ["src/**/*","resources/**/*"],
    "exclude": ["node_modules"],
    "paths": {
      "@/*": ["src/*"],
      "@resources/*": ["resources/*"]
    }
  }
